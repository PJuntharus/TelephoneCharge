@model IEnumerable<CalTelephoneCharge.Models.CellPhoneCharge>
@{
    ViewBag.Title = "CellPhone Charge";
}

<!DOCTYPE html>
<html>
<body>

    <h4>@ViewBag.Title</h4>
    <hr />

    <table class="table table-bordered table-condensed table-hover table-striped">

        <thead>
            <tr>
                <th>Mobile No.</th>
                <th>Charge</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Mobile_no)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PaymentTotal)
                </td>
                
            </tr>
            }

            </tbody>

    </table>

</body>


</html>





